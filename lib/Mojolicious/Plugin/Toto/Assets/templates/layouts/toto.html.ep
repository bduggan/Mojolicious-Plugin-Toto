<!DOCTYPE html>
<html lang="en">
<head>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title><%= title %></title>
%= base_tag
%= javascript '/js/jquery.js';
%= stylesheet '/bootstrap/css/bootstrap.min.css';
%= javascript '/bootstrap/js/bootstrap.min.js';
<style>
pre.toto_code {
    float:right;
    right:10%;
    padding:5px;
    border:1px grey dashed;
    font-family:monospace;
    position:absolute;
    }
</style>
</head>
<body>
<div class="row-fluid">
    <div class="span2">
        <div class="well" style="padding: 8px 0;">
            <ul class="nav nav-list">
                    <li style='width:100px;'>&nbsp;</li>
            % for my $c (objects) {
                    <li <%== $c eq current_object() ? q[ class="active"] : "" =%>>
                        <%= link_to $c => begin =%><%= $c =%><%= end =%>
                    </li>
            % }
            </ul>
        </div>
    </div>
    <div class="span10">
            % if (stash 'key') {
            %= include 'top_tabs_single';
            % } else {
            %= include 'top_tabs_plural';
            % }
            <div class="tab-content" style='width:auto'>
                     <%= content =%>
            </div>
        </div>
</div>

</html>

